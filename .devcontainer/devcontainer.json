{
    "name": "React Tic Tac Toe",
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "forwardPorts": [8000],
    "postStartCommand": "cd /workspaces/react-tic-tac-toe && python3 -m http.server 8000 &",
    "portsAttributes": {
        "8000": {
            "label": "Web App",
            "onAutoForward": "openBrowser",
            "visibility": "public",
            "protocol": "http"
        }
    },
    "remoteUser": "codespace",
    "hostRequirements": {
        "cpus": 2,
        "memory": "4gb",
        "storage": "32gb"
    }
}
