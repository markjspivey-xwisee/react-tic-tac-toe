{
    "name": "React Tic Tac Toe",
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "forwardPorts": [8000],
    "postCreateCommand": "chmod +x .devcontainer/startup.sh && .devcontainer/startup.sh &",
    "portsAttributes": {
        "8000": {
            "label": "Web App",
            "onAutoForward": "openBrowser",
            "visibility": "public"
        }
    }
}
